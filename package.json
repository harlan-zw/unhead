{
  "name": "unhead-monorepo",
  "version": "1.4.1",
  "private": "true",
  "packageManager": "pnpm@8.7.1",
  "description": "Typed utilities for defining, validating and building best-practice document &lt;head&gt;'s.",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/unjs/unhead#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unjs/unhead.git"
  },
  "bugs": {
    "url": "https://github.com/unjs/unhead/issues"
  },
  "keywords": [
    "meta tags",
    "node",
    "seo",
    "head"
  ],
  "scripts": {
    "build": "pnpm -r --filter=./packages/** run build",
    "stub": "pnpm -r --parallel --filter=./packages/** run stub",
    "test": "vitest",
    "export:sizes": "pnpm -r --parallel --filter=./packages/** run export:sizes",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "lint": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^21.1.2",
    "@vitest/ui": "^0.34.3",
    "@vue/server-renderer": "^3.3.4",
    "acorn-loose": "^8.3.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "utility-types": "^3.10.0",
    "vitest": "^0.34.3",
    "vue": "^3.3.4"
  },
  "resolutions": {
    "vue": "^3.3.4"
  }
}
