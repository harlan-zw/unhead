{
  "name": "unhead-monorepo",
  "type": "module",
  "version": "1.9.16",
  "private": "true",
  "packageManager": "pnpm@9.7.0",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unjs/unhead.git"
  },
  "scripts": {
    "build": "pnpm -r --filter=./packages/** run build",
    "stub": "pnpm -r --parallel --filter=./packages/** run stub",
    "test": "vitest",
    "test:memory": "vitest --expose-gc  run memory",
    "export:sizes": "pnpm -r --parallel --filter=./packages/** run export:sizes",
    "bump": "bumpp package.json packages/*/package.json --commit --push --tag",
    "release": "pnpm build && pnpm bump && pnpm -r publish --no-git-checks",
    "lint": "eslint . --fix",
    "benchmark": "vitest bench"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "acorn-loose": "^8.4.0",
    "bumpp": "^9.4.2",
    "eslint": "^9.9.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.1.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "utility-types": "^3.11.0",
    "vitest": "^2.0.5",
    "vue": "^3.4.37"
  },
  "resolutions": {
    "unhead": "workspace:*",
    "@unhead/vue": "workspace:*",
    "@unhead/dom": "workspace:*",
    "@unhead/ssr": "workspace:*",
    "@unhead/schema-org": "workspace:*",
    "vue": "^3.4.37",
    "shiki": "1.12.1",
    "nuxt": "3.12.4",
    "@nuxt/schema": "3.12.4"
  }
}
